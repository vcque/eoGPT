<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <intentionAction>
            <className>com.vcque.prompto.extensions.ultimate.actions.PromptoSqlAction</className>
            <category>Prompto</category>
        </intentionAction>
        <intentionAction>
            <className>com.vcque.prompto.extensions.ultimate.actions.PromptoMongoAction</className>
            <category>Prompto</category>
        </intentionAction>
    </extensions>
    <extensions defaultExtensionNs="com.vcque.prompto">
        <promptoRetriever implementation="com.vcque.prompto.extensions.ultimate.contexts.SqlSchemaRetriever" />
        <promptoRetriever implementation="com.vcque.prompto.extensions.ultimate.contexts.MongoDbSchemaRetriever" />
    </extensions>
</idea-plugin>